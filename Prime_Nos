        int n ,i=0,j=0,count=0,prime=0;
        printf("Enter no of prime no to be printed: ");
        scanf("%d",&n);
        
        for(i=2;i<=n+1;i++){
            prime=1;
            for(j=2;j*j<=i;j++){
                if(i%j==0){
                    prime=0;
                    break;
                }

            }
            if(prime){
                printf("%d\t",i);
                count++;
            }
 }
    



